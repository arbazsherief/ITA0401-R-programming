data <- read.csv("empid.csv")
print(data)
sal <- max(data$salary)
print(sal)
retval <- subset(data, salary == max(salary))
print(retval)
retval2<- subset( data, dept == "IT")
print(retval2)
info <- subset(data, salary > 600 & dept == "IT")
print(info)
retval3<- subset(data, as.Date(start_date) > as.Date("2014-01-01"))
print(retval3)